{
  "name": "aflow-stock-js",
  "version": "1.0.0",
  "description": "AFlow Node.js implementation for automated stock trading workflow generation",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node test/runExample.js",
    "example": "node test/runExample.js",
    "multisymbol": "node test/runMultiSymbol.js",
    "test": "node test/runExample.js",
    "dev": "node --watch test/runExample.js",
    "server": "node src/server.js",
    "frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "full": "concurrently \"npm run server\" \"npm run frontend\"",
    "setup": "npm install && cd frontend && npm install"
  },
  "keywords": [
    "aflow",
    "workflow",
    "stock",
    "trading",
    "ai",
    "agent",
    "mcts"
  ],
  "author": "AFlow Stock JS",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "technicalindicators": "^3.1.0",
    "node-cron": "^3.0.3",
    "cheerio": "^1.0.0-rc.12",
    "@anthropic-ai/sdk": "^0.9.0",
    "openai": "^4.20.0",
    "fs-extra": "^11.1.1",
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.2"
  }
}
